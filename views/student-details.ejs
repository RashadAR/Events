<!DOCTYPE html>
<html>

<head>
    <meta http-equiv="Content-Type" content="text/html; charset=UTF-8">
    <title>Eventle</title>
    <!-- Add CSS links here -->
</head>

<body>
    <!-- Include header file here -->

    <div class="content">
        <div class="container">
            <h1>Student details</h1>
            <% if (students.length > 0) { %>
            <table class="table table-hover">
                <tr>
                    <th>USN</th>
                    <th>Name</th>
                    <th>Branch</th>
                    <th>Sem</th>
                    <th>Email</th>
                    <th>Phone</th>
                    <th>College</th>
                    <th>Event</th>
                </tr>
                <% students.forEach(student => { %>
                <tr>
                    <td><%= student.usn %></td>
                    <td><%= student.name %></td>
                    <td><%= student.branch %></td>
                    <td><%= student.sem %></td>
                    <td><%= student.email %></td>
                    <td><%= student.phone %></td>
                    <td><%= student.college %></td>
                    <td><%= student.event_title %></td>
                </tr>
                <% }); %>
            </table>
            <% } else { %>
            <p>No result found</p>
            <% } %>
        </div>
    </div>

    <!-- Include footer file here -->
</body>

</html>
